{
  "name": "@zer0c14/ssh-tools",
  "version": "1.0.0",
  "description": "Some ssh tools (terminal, port forwarding, tunnel)",
  "main": "index.js",
  "types": "types/index.d.ts",
  "scripts": {
    "build": "rimraf dist && tsc -p tsconfig.build.json",
    "start": "ts-node -r tsconfig-paths/register src/index.ts",
    "start:dev": "tsc-watch -p tsconfig.build.json --onSuccess \"node dist/index.js\"",
    "start:debug": "tsc-watch -p tsconfig.build.json --onSuccess \"node --inspect-brk dist/index.js\"",
    "start:prod": "node dist/main.js",
    "format": "prettier --write \"src/**/*.ts\"",
    "lint": "tslint -p tsconfig.json -c tslint.json",
    "test": "mocha -r ts-node/register -r reflect-metadata/Reflect src/**/*.spec.ts",
    "test:cov": "nyc --extension .ts --include src --exclude src/**/*.spec.ts run-s test"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/zer0c14/node-ssh-tools.git"
  },
  "keywords": [
    "ssh",
    "terminal",
    "tunnel",
    "forwarding"
  ],
  "author": "Anton Makarov <zer0c14@gmail.com>",
  "contributors": [
    "Anton Makarov <zer0c14@gmail.com>"
  ],
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/zer0c14/node-ssh-tools/issues"
  },
  "homepage": "https://github.com/zer0c14/node-ssh-tools#readme",
  "dependencies": {
    "commander": "^3.0.2",
    "connection-string": "^3.0.4",
    "inversify": "^5.0.1",
    "pino": "^5.13.4",
    "reflect-metadata": "^0.1.13",
    "split2": "^3.1.1",
    "ssh2": "^0.8.5",
    "ssh2-promise": "^0.1.5",
    "ssh2-shell-server": "^2.2.3",
    "uuid": "^3.3.3"
  },
  "devDependencies": {
    "@types/chai": "^4.2.3",
    "@types/mocha": "^5.2.7",
    "@types/node": "^12.7.11",
    "@types/pino": "^5.8.10",
    "@types/split2": "^2.1.6",
    "@types/ssh2": "^0.5.39",
    "@types/uuid": "^3.4.5",
    "chai": "^4.2.0",
    "mocha": "^6.2.1",
    "npm-run-all": "^4.1.5",
    "nyc": "^14.1.1",
    "prettier": "^1.18.2",
    "rimraf": "^3.0.0",
    "ts-node": "^8.4.1",
    "tsc-watch": "^4.0.0",
    "tsconfig-paths": "^3.9.0",
    "tslint": "^5.20.0",
    "typescript": "^3.6.3"
  }
}
